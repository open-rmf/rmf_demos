<?xml version='1.0' ?>

<launch>
  <arg name="use_sim_time" default="true"/>

  <!-- Common launch -->
  <include file="$(find-pkg-share rmf_demos_base)/experimental_hotel.launch.xml">
    <arg name="use_sim_time" value="$(var use_sim_time)"/>
  </include>

  <!-- Simulation launch -->
  <include file="$(find-pkg-share rmf_demos_ignition)/simulation.launch.xml">
    <arg name="map_name" value="hotel" />
  </include>

</launch>
