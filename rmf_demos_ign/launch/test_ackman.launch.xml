<?xml version='1.0' ?>

<launch>
  <arg name="use_sim_time" default="true"/>
  <arg name="use_tpe" default="false"/>

  <!-- Common launch -->
  <include file="$(find-pkg-share rmf_demos)/test_ackman.launch.xml">
    <arg name="use_sim_time" value="true"/>
    <arg name="use_tpe" value="$(var use_tpe)"/>
  </include>

  <!-- Simulation launch -->
  <include file="$(find-pkg-share rmf_demos_ign)/simulation.launch.xml">
    <arg name="map_name" value="test_ackman" />
  </include>

</launch>
