<?xml version='1.0' ?>

<launch>
  <arg name="gazebo_version" default="11"/>
  <arg name="use_sim_time" default="true"/>

  <!-- Common launch -->
  <include file="$(find-pkg-share rmf_demos)/test_ackman.launch.xml">
    <arg name="use_sim_time" value="true"/>
  </include>

  <!-- Simulation launch -->
  <include file="$(find-pkg-share rmf_demos_gz)/simulation.launch.xml">
    <arg name="map_name" value="test_ackman" />
    <arg name="gazebo_version" value="$(var gazebo_version)" />
  </include>

</launch>
